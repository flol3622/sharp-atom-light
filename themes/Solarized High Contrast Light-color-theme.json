{
	"$schema": "vscode://schemas/color-theme",
	"type": "hcLight",
	"colors": {
		"activityBar.activeFocusBorder": "#252218",
		"activityBar.background": "#d2cbb5",
		"activityBar.inactiveForeground": "#2522187f",
		"activityBarBadge.background": "#545141",
		"badge.background": "#545141",
		"button.background": "#3b3832",
		"contrastActiveBorder": "#ff000000",
		"contrastBorder": "#3b3832",
		"editor.background": "#f8f4e5",
		"editorBracketHighlight.foreground1": "#ad9969",
		"editorBracketHighlight.foreground2": "#000",
		"editorBracketHighlight.foreground3": "#ad9969",
		"editorBracketHighlight.foreground4": "#000",
		"editorBracketHighlight.foreground5": "#ad9969",
		"editorBracketHighlight.foreground6": "#000",
		"editor.findMatchBackground": "#3b3832",
		"editor.foreground": "#110f08",
		"editor.lineHighlightBackground": "#fcf9ed",
		"editor.lineHighlightBorder": "#d2cbb5",
		"editor.selectionBackground": "#3b3832",
		"editorGroup.dropBackground": "#ff8c0ea5",
		"editorGutter.background": "#fcf9ed",
		"editorLineNumber.activeForeground": "#110f08",
		"editorLineNumber.foreground": "#b3aa99",
		"editorOverviewRuler.addedForeground": "#ff000000",
		"editorOverviewRuler.border": "#ff000000",
		"editorSuggestWidget.selectedBackground": "#ebeae7",
		"focusBorder": "#252218",
		"list.activeSelectionBackground": "#3b383220",
		"list.hoverBackground": "#3b383210",
		"list.inactiveSelectionBackground": "#3b383220",
		"menu.selectionBackground": "#ebeae7",
		"panel.background": "#f8f4e5",
		"quickInputList.focusBackground": "#ebeae7",
		"sideBar.background": "#ece6d3",
		"statusBar.background": "#d2cbb5",
		"statusBar.noFolderBackground": "#d2cbb5",
		"tab.activeBackground": "#3a3831",
		"tab.activeBorderTop": "#ff000000",
		"tab.activeForeground": "#fff",
		"tab.inactiveBackground": "#d2cbb5",
		"tab.unfocusedActiveBackground": "#3a3831a5",
		"tab.unfocusedActiveBorderTop": "#44423a00",
		"tab.unfocusedActiveForeground": "#fff",
		"terminal.background": "#3b3832",
		"terminal.foreground": "#fffbee",
		"titleBar.activeBackground": "#ECE6D3",
		"titleBar.inactiveBackground": "#ECE6D3"
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#110f08"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#CD3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7E7F8C",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#7E7F8C"
			}
		},
		{
			"scope": [
				"keyword.control",
				"storage"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "storage.type.function.arrow",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"string",
				"string.interpolated",
				"string.other",
				"string.quoted",
				"string.quoted.double",
				"string.quoted.other",
				"string.quoted.single",
				"string.quoted.triple",
				"string.regexp",
				"string.unquoted"
			],
			"settings": {
				"foreground": "#b02402"
			}
		},
		{
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#110f08"
			}
		},
		{
			"scope": [
				"variable.argument.css",
				"variable.scss"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "italic bold"
			}
		}
	]
}