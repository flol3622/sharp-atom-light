{
	"$schema": "vscode://schemas/color-theme",
	"type": "hcLight",
	"colors": {
		"activityBar.activeFocusBorder": "#252218",
		"activityBar.background": "#d2cbb5",
		"activityBar.inactiveForeground": "#2522187f",
		"activityBarBadge.background": "#545141",
		"badge.background": "#545141",
		"button.background": "#3a3831",
		"contrastActiveBorder": "#ff000000",
		"contrastBorder": "#3a3831",
		"editor.background": "#f8f4e5",
		"editorBracketHighlight.foreground1": "#ad9969",
		"editorBracketHighlight.foreground2": "#1a1606",
		"editorBracketHighlight.foreground3": "#ad9969",
		"editorBracketHighlight.foreground4": "#1a1606",
		"editorBracketHighlight.foreground5": "#ad9969",
		"editorBracketHighlight.foreground6": "#1a1606",
		"editorBracketMatch.background": "#FDFBF3",
		"editorBracketMatch.border": "#676357",
		"editor.findMatchBackground": "#3a3831",
		"editor.foreground": "#1a1606",
		"editor.lineHighlightBackground": "#fcf9ed",
		"editor.lineHighlightBorder": "#d2cbb5",
		"editor.selectionBackground": "#3a3831",
		"editorGroup.dropBackground": "#ff8c0ea5",
		"editorGutter.background": "#fcf9ed",
		"editorIndentGuide.activeBackground1": "#c6c4bd",
		"editorIndentGuide.activeBackground2": "#c6c4bd",
		"editorIndentGuide.activeBackground3": "#c6c4bd",
		"editorIndentGuide.activeBackground4": "#c6c4bd",
		"editorIndentGuide.activeBackground5": "#c6c4bd",
		"editorIndentGuide.activeBackground6": "#c6c4bd",
		"editorIndentGuide.background1": "#c6c4bd",
		"editorIndentGuide.background2": "#c6c4bd",
		"editorIndentGuide.background3": "#c6c4bd",
		"editorIndentGuide.background4": "#c6c4bd",
		"editorIndentGuide.background5": "#c6c4bd",
		"editorIndentGuide.background6": "#c6c4bd",
		"editorLineNumber.activeForeground": "#1a1606",
		"editorLineNumber.foreground": "#a69e8a",
		"editorOverviewRuler.addedForeground": "#ff000000",
		"editorOverviewRuler.border": "#ff000000",
		"editorStickyScroll.background": "#FDFBF3",
		"editorStickyScrollHover.background": "#e8e6df",
		"editorSuggestWidget.selectedBackground": "#ebeae7",
		"focusBorder": "#252218",
		"list.activeSelectionBackground": "#3a383120",
		"list.hoverBackground": "#3a383110",
		"list.inactiveSelectionBackground": "#3a383120",
		"menu.selectionBackground": "#ebeae7",
		"panel.background": "#f8f4e5",
		"quickInputList.focusBackground": "#ebeae7",
		"sideBar.background": "#ece6d3",
		"statusBar.background": "#d2cbb5",
		"statusBar.noFolderBackground": "#d2cbb5",
		"tab.activeBackground": "#3a3831",
		"tab.activeBorderTop": "#ff000000",
		"tab.activeForeground": "#fff",
		"tab.inactiveBackground": "#d2cbb5",
		"tab.unfocusedActiveBackground": "#3a3831a5",
		"tab.unfocusedActiveBorderTop": "#44423a00",
		"tab.unfocusedActiveForeground": "#fff",
		"terminal.background": "#3a3831",
		"terminal.foreground": "#FDFBF3",
		"titleBar.activeBackground": "#ece6d3",
		"titleBar.inactiveBackground": "#ece6d3",
		"tree.indentGuidesStroke": "#676357",
		"tree.inactiveIndentGuidesStroke": "#67635780"
	},
	"tokenColors": [
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#1a1606"
			}
		},
		{
			"scope": [
				"strong",
				"markup.bold",
				"markup.heading",
				"keyword.control",
				"storage"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#423d2b"
			}
		},
		{
			"scope": [
				"emphasis",
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"markup.underline",
				"variable.argument.css",
				"variable.scss"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.strikethrough",
			"settings": {
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#CD3131"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#800080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#7E7F8C",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#7E7F8C"
			}
		},
		{
			"scope": "storage.type.function.arrow",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"string",
				"string.interpolated",
				"string.other",
				"string.quoted",
				"string.quoted.double",
				"string.quoted.other",
				"string.quoted.single",
				"string.quoted.triple",
				"string.regexp",
				"string.unquoted"
			],
			"settings": {
				"foreground": "#b02402"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": "italic bold"
			}
		}
	]
}